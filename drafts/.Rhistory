gather(Endogène, Side, Exogène, key = prov, value = prop, factor_key = TRUE)
to_plot <- bind_rows(ploter, plotps, ploteh, ploterref, plotpsref, plotehref)
to_plot %>% ggplot +
aes(x = Date) +
geom_area(aes(y = prop, fill = prov), alpha = 0.5) +
